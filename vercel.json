{
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/build",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": null,
  "rewrites": [
    {
      "source": "/p/:code",
      "destination": "/?code=:code"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
